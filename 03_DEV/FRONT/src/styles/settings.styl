input

  -webkit-user-select: auto !important;
  -khtml-user-select: auto !important;
  -moz-user-select: auto !important;
  -ms-user-select: auto !important;
  user-select: auto !important;

html

  width 100vw
  overflow-x hidden

body

  margin 0px
  padding 0px
  top 0px
  left 0px
  right 0px
  bottom 0px
  width 100vw
  font-family $fprimary

#Site

  min-height 100vh
  min-height calc(100vh - var(--vh, 0px));

h2

  font-weight bold
  font-size: 100px;
  line-height 90px
  margin 0px
  margin-bottom 40px

  @media $large

      font-size 90px
      line-height 80px

  @media $small

      font-size 60px
      line-height 70px

  small

    display block
    font-weight 500
    font-size: 45px;
    line-height: 35px;

    @media $large

        font-size 35px
        line-height 25px

    @media $small

        font-size 25px
        line-height 15px

h3

  font-weight 500;
  font-size: 36px;
  letter-spacing: 0;
  line-height: 30px;
  margin 0px 0px

  @media $small

    font-size 26px
    line-height 20px

h4

  font-weight 500;
  font-size: 16px;
  letter-spacing: 0;
  line-height: 20px;
  color $primary
  margin 0px 0px

p

  font-size: 14px;
  margin 0px 0px
  color: $black;
  letter-spacing: 0;

p.big

  font-size: 18px;
  line-height: 28px;

  @media $small

    font-size 16px
    line-height 26px

p.light

  color $black!important

.ps__rail-y

    background-color transparent!important

.ps__thumb-y

    background-color $grey!important

@font-face {
    font-family: 'Euclid Square';
    src: url('/assets/fonts/EuclidSquare-Bold.eot');
    src: local('Euclid Square Bold'), local('/assets/fonts/EuclidSquare-Bold'),
        url('/assets/fonts/EuclidSquare-Bold.eot?#iefix') format('embedded-opentype'),
        url('/assets/fonts/EuclidSquare-Bold.woff2') format('woff2'),
        url('/assets/fonts/EuclidSquare-Bold.woff') format('woff'),
        url('/assets/fonts/EuclidSquare-Bold.ttf') format('truetype');
